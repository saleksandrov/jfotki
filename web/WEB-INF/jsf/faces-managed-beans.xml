<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>

  <managed-bean>
    <description>Backing bean that contains pictures informaiton.</description>
    <managed-bean-name>pictureBean</managed-bean-name>
    <managed-bean-class>com.asv.jfotki.web.bean.PictureBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <description>Backing bean that contains album informaiton.</description>
    <managed-bean-name>albumBean</managed-bean-name>
    <managed-bean-class>com.asv.jfotki.web.bean.AlbumBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <description>Secirity</description>
    <managed-bean-name>securityBean</managed-bean-name>
    <managed-bean-class>com.asv.jfotki.web.bean.SecurityBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

    <managed-bean>
      <description>RegistrationBean</description>
      <managed-bean-name>registrationBean</managed-bean-name>
      <managed-bean-class>com.asv.jfotki.web.bean.RegistrationBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

</faces-config>
